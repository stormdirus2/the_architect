{
  "type": "origins:multiple",
  "take_damage": {
    "type": "origins:damage_over_time",
    "interval": 4,
    "damage": 2,
    "damage_source": {
      "name": "onFire",
      "fire": true,
      "bypasses_armor": true,
      "unblockable": true
    },
    "condition": {
      "type": "thearchitect:touching_gold"
    }
  },
  "shake": {
    "type": "origins:shaking",
    "condition": {
      "type": "thearchitect:touching_gold"
    }
  },
  "no_eat": {
    "type": "origins:prevent_item_use",
    "item_condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:food"
        },
        {
          "type": "origins:ingredient",
          "ingredient": {
            "tag": "thearchitect:gold"
          }
        }
      ]
    }
  },
  "name": "Aurophobia",
  "description": "You take massive damage over time while in contact with gold. You cannot consume gold-related items."
}